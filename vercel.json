{
  "name": "rx-magazine-admin",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://rx-magazine-backend.onrender.com",
    "VITE_API_URL_PROD": "https://rx-magazine-backend.onrender.com",
    "VITE_ADMIN_EMAIL": "admin@rxlifestyle.com",
    "VITE_ADMIN_PASSWORD": "rxacademy2025",
    "VITE_NODE_ENV": "production"
  }
}
